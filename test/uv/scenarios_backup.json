{
    "scenario_empty_python": {
        "image": "mcr.microsoft.com/devcontainers/python:3",
        "features": {
            "uv": {}
        }
    },
    "scenario_auto_python": {
        "image": "mcr.microsoft.com/devcontainers/python:3",
        "features": {
            "uv": {
                "installationMethod": "auto"
            }
        }
    },
    "scenario_pipx_python": {
        "image": "mcr.microsoft.com/devcontainers/python:3",
        "features": {
            "uv": {
                "installationMethod": "pipx"
            }
        }
    },
    "scenario_script_python": {
        "image": "mcr.microsoft.com/devcontainers/python:3",
        "features": {
            "uv": {
                "installationMethod": "script"
            }
        }
    },
    "scenario_empty_mu": {
        "image": "registry.gitlab.com/mu-bd-ce/devcontainers/python",
        "features": {
            "ghcr.io/rocker-org/devcontainer-features/apt-packages:1": {
                "packages": "curl"
            },
            "uv": {}
        }
    },
    "scenario_auto_mu": {
        "image": "registry.gitlab.com/mu-bd-ce/devcontainers/python",
        "features": {
            "ghcr.io/rocker-org/devcontainer-features/apt-packages:1": {
                "packages": "curl"
            },
            "uv": {
                "installationMethod": "auto"
            }
        }
    },
    "scenario_pipx_mu": {
        "image": "registry.gitlab.com/mu-bd-ce/devcontainers/python",
        "features": {
            "ghcr.io/devcontainers/features/python:1": {
                "version": "os-provided",
                "installTools": true,
                "installJupyterlab": false
            },
            "uv": {
                "installationMethod": "pipx"
            }
        }
    },
    "scenario_script_mu": {
        "image": "registry.gitlab.com/mu-bd-ce/devcontainers/python",
        "features": {
            "ghcr.io/rocker-org/devcontainer-features/apt-packages:1": {
                "packages": "curl"
            },

            "uv": {
                "installationMethod": "script"
            }
        }
    }
}